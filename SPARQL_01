 
getPublicationsPublishedInYear
#It returns a data frame with all the publications (i.e. the rows)
# that have been published in the input year (e.g. 2020).
PREFIX GOT: <https://schema.org/datePublished>

SELECT ?ID ?Title

WHERE {
?publication GOT:col-got-publicationYear ?2020  .
?publication GOT:col-got-publicationYear ?PublicationYear  .

}

getPublicationsByAuthorId
#It returns a data frame with all the publications (i.e. the rows) 
# that have been authored by the person having the identifier specified 
# as input (e.g. "0000-0001-9857-1511").

PREFIX GOT: <https://github.com/lelax/D_Sign_Data/blob/main/UMLclasses/isWrittenBy>

SELECT ?ID ?Title

WHERE {
?publication GOT:col-got-authorID ?"0000-0001-9857-1511"  .
?publication GOT:col-got-authorID ?authorID  .

}

getMostCitedPublication 
#It returns a data frame with all the publications (i.e. the rows) 
# that have received the most number of citations by other publications.

PREFIX GOT: <https://github.com/lelax/D_Sign_Data/blob/main/UMLclasses/isCitedBy>

SELECT ?ID ?Title

WHERE {
?publication GOT:col-got-cite ?_____  .
?publication GOT:col-got-title ?___  .

}
limit 10

getMostCitedVenue 
#It returns a data frame with all the venues (i.e. the rows) containing 
# the publications that, overall, have received the most number of 
# citations by other publications.

PREFIX GOT: <https://github.com/lelax/D_Sign_Data/blob/main/UMLclasses/isCitedBy>

SELECT ?ID ?Title

WHERE {
?venue GOT:col-got-cite ?_____  .
?venue GOT:col-got-title ?___  .

}
limit 10

getVenuesByPublisherId 
#It returns a data frame with all the venues (i.e. the rows) 
# that have been published by the organisation having the identifier 
# specified as input (e.g. "crossref:78").

getPublicationInVenue: 
#It returns a data frame with all the publications (i.e. the rows) 
#that have been included in the venue having the identifier specified as input (e.g. "issn:0944-1344").

PREFIX GOT: <https://schema.org/isPartOf>

SELECT ?ID ?Title

WHERE {
?publication GOT:col-got-venue ?"issn:0944-1344" .
?publication GOT:col-got-venue ?venue .

}

getJournalArticlesInIssue: 
#It returns a data frame with all the journal articles (i.e. the rows) that have been included in the input issue (e.g. "9") of the input volume (e.g. "17") of the journal having the identifier specified as input (e.g. "issn:2164-5515").

PREFIX GOT: <https://schema.org/ScholarlyArticle>

SELECT ?ID ?Title

WHERE {
?JournalArticle GOT:col-got-issue ?9 .
?JournalArticle GOT:col-got-issue ?issue .
?JournalArticle GOT:col-got-volume ?17 .
?JournalArticle GOT:col-got-volume ?volume .
?JournalArticle GOT:col-got-journal ?"issn:2164-5515" .
?JournalArticle GOT:col-got-journal ?journal .

}

getJournalArticlesInVolume: 
#It returns a data frame with all the journal articles (i.e. the rows) that #have been included, independently from the issue, in input volume (e.g. "17") #of the journal having the identifier specified as input (e.g. "issn:2164-5515").

PREFIX GOT: <https://schema.org/ScholarlyArticle>

SELECT ?ID ?Title

WHERE {
?JournalArticle GOT:col-got-volume ?17 .
?JournalArticle GOT:col-got-volume ?volume .
?JournalArticle GOT:col-got-journal ?"issn:2164-5515" .
?JournalArticle GOT:col-got-journal ?journal .

}

getJournalArticlesInJournal: 
#It returns a data frame with all the journal articles (i.e. the rows) that #have been included, independently from the issue and the volume, in the #journal having the identifier specified as input (e.g. "issn:2164-5515").

PREFIX GOT: <https://schema.org/ScholarlyArticle>

SELECT ?ID ?Title

WHERE {
?JournalArticle GOT:col-got-journal ?"issn:2164-5515" .
?JournalArticle GOT:col-got-journal ?journal .

}

getProceedingsByEvent: 
#It returns a data frame with all the proceedings (i.e. the rows) that refer #to the events that match (in lowercase), even partially, with the name #specified as input (e.g. "web").

PREFIX GOT: <http://purl.org/spar/fabio/AcademicProceedings>

SELECT ?ID ?Title

WHERE {
    ?Proceedings GOT:col-got-event ?event  .
filter contains(?event,"web") 

}

getPublicationAuthors: 
#It returns a data frame with all the authors (i.e. the rows) of the #publication with the identifier specified as input (e.g. "doi:10.1080/21645515.2021.1910000").

PREFIX GOT: <https://schema.org/Person>

SELECT ?name ?GivenName ?FamilyName

WHERE {
?publication GOT:col-got-id ?"doi:10.1080/21645515.2021.1910000"  .
?publication GOT:col-got-id ?id .

}


getPublicationsByAuthorName: 
#It returns a data frame with all the publications (i.e. the rows) that have #been authored by the people having their name matching (in lowercase), even #partially, with the name specified as input (e.g. "doe").

PREFIX GOT: <https://schema.org/Publication>

SELECT ?id ?Title

WHERE {
  ?author GOT:col-got-name ?name .
filter contains(?name,"doe") 

}

getDistinctPublisherOfPublications: 
#It returns a data frame with all the distinct publishers (i.e. the rows) that #have published the venues of the publications with identifiers those #specified as input (e.g. [ "doi:10.1080/21645515.2021.1910000", "doi:10.3390/ijfs9030035" ]).

PREFIX GOT: <https://schema.org/publishedBy>

SELECT ?id ?Name

WHERE {
      ?publication GOT:col-got-Venueid ?"doi:10.1080/21645515.2021.1910000" .
  &&  ?publication GOT:col-got-Venueid ?"doi:10.3390/ijfs9030035" .
      ?publication GOT:col-got-Venueid ?Venueid  .

}
